{
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://localhost:5000"
            }
        }
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} - {Level:u} - {Message:lj}{NewLine}{Exception}"
                }
            },
            {
                "Name": "File",
                "Args": {
                    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} - {Level:u} - {Message:lj}{NewLine}{Exception}"
                }
            }
        ]
    },
    "Baralho": {
        "Duelo": {
            "Canhao": 20,
            "Timoneiro": 5
        },
        "DueloSurpresa": {
            "AtaqueSurpresa": 0
        },
        "Embarcacao": {
            "CascoAco": 1,
            "GuerrilhaNaval": 1,
            "HolandesAlado": 1,
            "MercadorTortuga": 1,
            "OlhoCiclope": 1,
            "OuricoInfernal": 1,
            "RodaFortuna": 1,
            "ServoPoseidon": 1,
            "VossaAlteza": 1
        },
        "Evento": {
            "Kraken": 0
        },
        "Passivo": {
            "BauArmadilha": 0
        },
        "ResolucaoImediata": {
            "ConvocarTripulacao": 4,
            "Explosivos": 0,
            "HomemAoMar": 5,
            "Luneta": 5,
            "MapaDoTesouro": 6,
            "Mergulhador": 0,
            "Papagaio": 8,
            "Rum": 4,
            "Saque": 10
        },
        "Tesouro": 18,
        "Tripulacao": {
            "Pirata": 8,
            "PirataAmaldicoado": 4,
            "PirataFantasma": 4,
            "PirataNobre": 0
        }
    }
}
